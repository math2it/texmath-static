{
    "name": "eleventy-base-blog",
    "version": "0.1.0",
    "description": "11ty version of texmath",
    "credit": "Customized from eleventy-base-blog version 5.02",
    "scripts": {
        "start": "npm-run-all --parallel dev:*",
        "build": "run-s prod:*",
        "dev:eleventy": "eleventy --serve",
        "dev:parcel": "parcel watch ./_assets/js/main.js --out-dir ./_site/assets",
        "prod:eleventy": "eleventy",
        "prod:parcel": "parcel build ./_assets/js/main.js --out-dir ./_site/assets",
        "debug": "DEBUG=* eleventy"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/math2it/texmath-static"
    },
    "author": {
        "name": "Math2IT Team",
        "email": "dinhanhthi@gmail.com",
        "url": "https://math2it.com/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/math2it/texmath-static/issues"
    },
    "homepage": "https://texmath.com",
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.0.9",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "luxon": "^1.21.3",
        "markdown-it": "^8.4.2",
        "markdown-it-anchor": "^5.2.5",
        "npm-run-all": "^4.1.5",
        "sass": "^1.27.0"
    },
    "dependencies": {
        "parcel-bundler": "^1.12.4"
    }
}
